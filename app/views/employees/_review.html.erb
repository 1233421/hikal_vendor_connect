<div class="row">
	<div class="col-6">
		<div class="page-header pt-3 pb-2 mb-3">
		  <h5>General Information</h5>
		</div>

		<% general_info = @vendor_application.general_information %>
		<table class="table table-striped">
			<tbody>
				<tr>
					<th>Vendor Type</th>
					<td><%= general_info.vendor_type %></td>
				</tr>
				<tr>
					<th>Vendor Sub Type</th>
					<td><%= general_info.vendor_sub_type %></td>
				</tr>
				<tr>
					<th>Nature of Entiry</th>
					<td><%= general_info.nature_of_the_entity %></td>
				</tr>
				<tr>
					<th>Annual Sales</th>
					<td><%= general_info.annual_sales %></td>
				</tr>
				<tr>
					<th>List of sales</th>
					<td><%= general_info.list_of_products %></td>
				</tr>

				<tr>
					<th>
						Correspondence Address
					</th>
					<td>
						<%= general_info.correspondence_address_line_1 %>,<br/>
						<%= general_info.correspondence_address_line_2 %>,<br/>
						<%= general_info.correspondence_city %>, <%= general_info.correspondence_state %>,<br/>
						<%= general_info.correspondence_country %>, <%= general_info.correspondence_pin_code %>
					</td>
				</tr>

				<tr>
					<th>
						Shipping Address
					</th>
					<td>
						<%= general_info.shipping_address_line_1 %>,<br/>
						<%= general_info.shipping_address_line_2 %>,<br/>
						<%= general_info.shipping_city %>, <%= general_info.shipping_state %>,<br/>
						<%= general_info.shipping_country %>, <%= general_info.shipping_pin_code %>
