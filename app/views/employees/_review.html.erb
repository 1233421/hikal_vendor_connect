		<div class="page-header pt-3 pb-2 mb-3">
		  <h5>GST Information</h5>
		</div>

		<% gst_info = @vendor_application.gst_info %>
		<table class="table table-striped">
			<tbody>
				<tr>
					<th>PAN card</th>
					<td><%= gst_info.pan_card %></td>
					<td><%= image_tag url_for(gst_info.pan_card_doc), style: "width: 200px;height: 200px;" %></td>
				</tr>
				<tr>
					<th>Is your organization registered with GSTN</th>
					<td colspan="2"><%= GstInfo::DROPDOWN_OPTIONS.get_by_key_from_2d_array(gst_info.is_organization_registered) %></td>
				</tr>
				<tr>
					<th>Is your business category exempted from GSTN</th>
					<td colspan="2"><%= GstInfo::DROPDOWN_OPTIONS.get_by_key_from_2d_array(gst_info.is_gst_exempted) %></td>
				</tr>
				<tr>
					<th>Is annual turnover below Rs. 20 lakh</th>
					<td colspan="2"><%= GstInfo::DROPDOWN_OPTIONS.get_by_key_from_2d_array(gst_info.is_turonover_below_par) %></td>
				</tr>

				<% if gst_info.is_gst_number_present? %>
					<tr>
						<th>GST No</th>
						<td colspan="2"><%= gst_info.gst_no %></td>
					</tr>
					<tr>
						<th>Registered Email</th>
						<td colspan="2"><%= gst_info.registered_email %></td>
					</tr>
					<tr>
